{
  "indicators": [
    {
      "id": "ema",
      "label": "Exponential Moving Average",
      "blurb": "Exponential Moving Average — responsive trend line. Common for crossovers and pullbacks.",
      "params": {
        "period": { "type": "int", "default": 20, "min": 2, "max": 400 },
        "source": { "type": "enum", "default": "close", "options": ["close", "open", "high", "low"] }
      },
      "outputs": ["value"]
    },
    {
      "id": "sma",
      "label": "Simple Moving Average",
      "blurb": "Simple Moving Average — classic trend average; slower than EMA.",
      "params": {
        "period": { "type": "int", "default": 20, "min": 2, "max": 400 },
        "source": { "type": "enum", "default": "close", "options": ["close", "open", "high", "low"] }
      },
      "outputs": ["value"]
    },
    {
      "id": "rsi",
      "label": "Relative Strength Index",
      "blurb": "Relative Strength Index — momentum/overbought-oversold (0–100).",
      "params": {
        "length": { "type": "int", "default": 14, "min": 2, "max": 50 }
      },
      "outputs": ["value"]
    },
    {
      "id": "macd",
      "label": "MACD",
      "blurb": "MACD — momentum via fast/slow EMA difference + signal line.",
      "params": {
        "fast": { "type": "int", "default": 12, "min": 1, "max": 50 },
        "slow": { "type": "int", "default": 26, "min": 1, "max": 100 },
        "signal": { "type": "int", "default": 9, "min": 1, "max": 50 }
      },
      "outputs": ["line", "signal", "hist"]
    },
    {
      "id": "stoch",
      "label": "Stochastic",
      "blurb": "Stochastic — momentum oscillator based on HH/LL.",
      "params": {
        "k": { "type": "int", "default": 14, "min": 1, "max": 50 },
        "d": { "type": "int", "default": 3, "min": 1, "max": 20 },
        "smooth": { "type": "int", "default": 3, "min": 1, "max": 20 }
      },
      "outputs": ["k", "d"]
    },
    {
      "id": "atr",
      "label": "Average True Range",
      "blurb": "Average True Range — volatility measure used for stops/targets.",
      "params": {
        "length": { "type": "int", "default": 14, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "bb",
      "label": "Bollinger Bands",
      "blurb": "Bollinger Bands — mean + std dev envelope.",
      "params": {
        "length": { "type": "int", "default": 20, "min": 2, "max": 100 },
        "mult": { "type": "float", "default": 2.0, "min": 0.1, "max": 5.0 },
        "source": { "type": "enum", "default": "close", "options": ["close", "open", "high", "low"] }
      },
      "outputs": ["basis", "upper", "lower"]
    },
    {
      "id": "vwap",
      "label": "Volume-Weighted Average Price",
      "blurb": "Volume-Weighted Average Price — fair value anchor.",
      "params": {
        "session": { "type": "enum", "default": "24h", "options": ["24h", "day", "week"] }
      },
      "outputs": ["value"]
    },
    {
      "id": "obv",
      "label": "On-Balance Volume",
      "blurb": "On-Balance Volume — cumulative volume momentum.",
      "params": {},
      "outputs": ["value"]
    },
    {
      "id": "cci",
      "label": "Commodity Channel Index",
      "blurb": "Commodity Channel Index — deviation from mean; reversal timing.",
      "params": {
        "length": { "type": "int", "default": 20, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "adx",
      "label": "Average Directional Index",
      "blurb": "Average Directional Index — trend strength (0–100).",
      "params": {
        "length": { "type": "int", "default": 14, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "keltner",
      "label": "Keltner Channels",
      "blurb": "Keltner Channels — EMA envelope via ATR.",
      "params": {
        "length": { "type": "int", "default": 20, "min": 1, "max": 100 },
        "atrLen": { "type": "int", "default": 10, "min": 1, "max": 100 },
        "mult": { "type": "float", "default": 1.5, "min": 0.1, "max": 5.0 }
      },
      "outputs": ["basis", "upper", "lower"]
    },
    {
      "id": "donchian",
      "label": "Donchian Channels",
      "blurb": "Donchian Channels — HH/LL channel breakout levels.",
      "params": {
        "upperLen": { "type": "int", "default": 20, "min": 1, "max": 100 },
        "lowerLen": { "type": "int", "default": 10, "min": 1, "max": 100 }
      },
      "outputs": ["upper", "lower", "mid"]
    },
    {
      "id": "ichimoku",
      "label": "Ichimoku Cloud",
      "blurb": "Ichimoku Cloud — full trend system (tenkan, kijun, spanA, spanB).",
      "params": {
        "tenkan": { "type": "int", "default": 9, "min": 1, "max": 50 },
        "kijun": { "type": "int", "default": 26, "min": 1, "max": 100 },
        "spanB": { "type": "int", "default": 52, "min": 1, "max": 200 }
      },
      "outputs": ["tenkan", "kijun", "spanA", "spanB"]
    },
    {
      "id": "supertrend",
      "label": "Supertrend",
      "blurb": "Supertrend — volatility-based trailing stop/flip.",
      "params": {
        "atrLen": { "type": "int", "default": 10, "min": 1, "max": 100 },
        "mult": { "type": "float", "default": 3.0, "min": 0.1, "max": 10.0 }
      },
      "outputs": ["line", "trend"]
    },
    {
      "id": "mfi",
      "label": "Money Flow Index",
      "blurb": "Money Flow Index — volume-weighted RSI.",
      "params": {
        "length": { "type": "int", "default": 14, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "roc",
      "label": "Rate of Change",
      "blurb": "Rate of Change — momentum % change.",
      "params": {
        "length": { "type": "int", "default": 12, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "williamsR",
      "label": "Williams %R",
      "blurb": "Williams %R — fast overbought/oversold (-100 to 0).",
      "params": {
        "length": { "type": "int", "default": 14, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    },
    {
      "id": "psar",
      "label": "Parabolic SAR",
      "blurb": "Parabolic SAR — trend-following dots for entries/trailing.",
      "params": {
        "afStart": { "type": "float", "default": 0.02, "min": 0.01, "max": 0.1 },
        "afStep": { "type": "float", "default": 0.02, "min": 0.01, "max": 0.1 },
        "afMax": { "type": "float", "default": 0.2, "min": 0.1, "max": 1.0 }
      },
      "outputs": ["value"]
    },
    {
      "id": "volma",
      "label": "Volume MA",
      "blurb": "Volume MA — moving average of volume.",
      "params": {
        "period": { "type": "int", "default": 20, "min": 1, "max": 100 }
      },
      "outputs": ["value"]
    }
  ],
  "strategies": [
    {
      "id": "ema_crossover_pro",
      "label": "EMA Crossover Pro",
      "blurb": "Enter when fast EMA crosses above slow EMA; ATR stop, trailing TP.",
      "directionDefault": "long",
      "allowedIndicators": ["ema", "atr", "sma", "rsi"],
      "allowedOperators": [">", ">=", "<", "<=", "==", "crosses_above", "crosses_below", "rising", "falling", "within", "outside"],
      "exitDefaults": { "useATRStop": true, "atrMult": 2.0, "trailingTPPct": 0.7 }
    },
    {
      "id": "ema200_pullback",
