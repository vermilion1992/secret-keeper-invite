{
  "global": {
    "ruleCap": 5,
    "operators": [
      {
        "key": "crosses_above",
        "label": "Crosses Above",
        "tooltip": "Triggers when the left value crosses above the right value"
      },
      {
        "key": "crosses_below", 
        "label": "Crosses Below",
        "tooltip": "Triggers when the left value crosses below the right value"
      },
      {
        "key": "is_above",
        "label": "Is Above", 
        "tooltip": "Triggers when the left value is above the right value"
      },
      {
        "key": "is_below",
        "label": "Is Below",
        "tooltip": "Triggers when the left value is below the right value"
      },
      {
        "key": "is_true",
        "label": "Is True",
        "tooltip": "Triggers when the condition is true"
      }
    ],
    "combiners": [
      {
        "key": "AND",
        "label": "AND",
        "tooltip": "All conditions must be true (stricter)"
      },
      {
        "key": "OR", 
        "label": "OR",
        "tooltip": "Any condition can be true (looser)"
      }
    ],
    "directions": [
      {
        "key": "long_only",
        "label": "Long Only"
      },
      {
        "key": "short_only", 
        "label": "Short Only"
      },
      {
        "key": "both",
        "label": "Both"
      }
    ],
    "tooltips": {
      "entryDirection": "Choose whether to trade long positions, short positions, or both",
      "entryInverse": "When enabled, the strategy will enter when conditions are NOT met",
      "combiner": "Choose how to combine multiple entry conditions"
    },
    "blurbs": {
      "indicatorSettings": "Configure the parameters for each indicator used in this strategy",
      "entryConditions": "Define when the strategy should enter a trade"
    }
  },
  "strategies": {
    "ema-crossover": {
      "indicatorSettings": {
        "ema": {
          "fields": ["fast", "slow", "maType"]
        }
      },
      "tiles": ["ema"],
      "operands": {
        "ema": ["ema_fast", "ema_slow", "price"]
      },
      "defaultSeeds": [
        {
          "lhs": "ema_fast",
          "op": "crosses_above", 
          "rhs": "ema_slow"
        }
      ]
    },
    "rsi-mean-reversion": {
      "indicatorSettings": {
        "rsi": {
          "fields": ["length", "overbought", "oversold"]
        },
        "atr": {
          "fields": ["length"]
        }
      },
      "tiles": ["rsi", "atr"],
      "operands": {
        "rsi": ["rsi", "rsi_50", "rsi_overbought", "rsi_oversold"],
        "atr": ["atr_gt_threshold"]
      },
      "defaultSeeds": [
        {
          "lhs": "rsi",
          "op": "is_below",
          "rhs": "rsi_oversold"
        },
        {
          "lhs": "atr_gt_threshold",
          "op": "is_true"
        }
      ]
    },
    "macd-confirmation": {
      "indicatorSettings": {
        "macd": {
          "fields": ["fast", "slow", "signal"]
        },
        "ema": {
          "fields": ["period", "maType"]
        }
      },
      "tiles": ["macd", "ema"],
      "operands": {
        "macd": ["macd_line", "macd_signal", "macd_hist_gt0"],
        "ema": ["price", "ema"]
      },
      "defaultSeeds": [
        {
          "lhs": "macd_line",
          "op": "crosses_above",
          "rhs": "macd_signal"
        },
        {
          "lhs": "price",
          "op": "is_above", 
          "rhs": "ema"
        }
      ]
    },
    "bollinger-bounce": {
      "indicatorSettings": {
        "bb": {
          "fields": ["period", "stdDev"]
        },
        "rsi": {
          "fields": ["length", "overbought", "oversold"]
        }
      },
      "tiles": ["bollinger", "rsi"],
      "operands": {
        "bollinger": ["price", "bb_upper", "bb_lower", "bb_mid"],
        "rsi": ["rsi", "rsi_50", "rsi_overbought", "rsi_oversold"]
      },
      "defaultSeeds": [
        {
          "lhs": "price",
          "op": "is_below",
          "rhs": "bb_lower"
        },
        {
          "lhs": "rsi",
          "op": "is_below",
          "rhs": "rsi_oversold"
        }
      ]
    },
    "stochastic-swing": {
      "indicatorSettings": {
        "stoch": {
          "fields": ["k", "d", "overbought", "oversold"]
        },
        "sma": {
          "fields": ["period"]
        }
      },
      "tiles": ["stochastic", "sma"],
      "operands": {
        "stochastic": ["stoch_k", "stoch_d", "stoch_ob", "stoch_os"],
        "sma": ["price", "sma"]
      },
      "defaultSeeds": [
        {
          "lhs": "stoch_k",
          "op": "crosses_above",
          "rhs": "stoch_d"
        },
        {
          "lhs": "price",
          "op": "is_above",
          "rhs": "sma"
        }
      ]
    },
    "atr-trailing": {
      "indicatorSettings": {
        "ema": {
          "fields": ["period", "maType"]
        },
        "atr": {
          "fields": ["period", "multiplier"]
        }
      },
      "tiles": ["ema", "atr"],
      "operands": {
        "ema": ["price", "ema"],
        "atr": ["atr_gt_threshold"]
      },
      "defaultSeeds": [
        {
          "lhs": "price",
          "op": "is_above",
          "rhs": "ema"
        },
        {
          "lhs": "atr_gt_threshold",
          "op": "is_true"
        }
      ]
    },
    "vwap-trend": {
      "indicatorSettings": {
        "vwap": {
          "fields": []
        },
        "rsi": {
          "fields": ["length", "overbought", "oversold"]
        }
      },
      "tiles": ["vwap", "rsi"],
      "operands": {
        "vwap": ["price", "vwap"],
        "rsi": ["rsi", "rsi_50", "rsi_overbought", "rsi_oversold"]
      },
      "defaultSeeds": [
        {
          "lhs": "price",
          "op": "is_above",
          "rhs": "vwap"
        },
        {
          "lhs": "rsi",
          "op": "is_above",
          "rhs": "rsi_50"
        }
      ]
    }
  },
  "operandLabels": {
    "ema_fast": "EMA Fast",
    "ema_slow": "EMA Slow",
    "ema": "EMA",
    "price": "Price",
    "rsi": "RSI",
    "rsi_50": "RSI 50 Line",
    "rsi_overbought": "RSI Overbought",
    "rsi_oversold": "RSI Oversold", 
    "atr_gt_threshold": "ATR > Threshold",
    "macd_line": "MACD Line",
    "macd_signal": "MACD Signal",
    "macd_hist_gt0": "MACD Histogram > 0",
    "bb_upper": "BB Upper",
    "bb_lower": "BB Lower",
    "bb_mid": "BB Middle",
    "stoch_k": "Stochastic %K",
    "stoch_d": "Stochastic %D",
    "stoch_ob": "Stochastic Overbought",
    "stoch_os": "Stochastic Oversold",
    "sma": "SMA",
    "vwap": "VWAP"
  },
  "indicatorLabels": {
    "ema": "EMA",
    "rsi": "RSI", 
    "atr": "ATR",
    "macd": "MACD",
    "bollinger": "Bollinger Bands",
    "stochastic": "Stochastic",
    "sma": "SMA",
    "vwap": "VWAP"
  }
}